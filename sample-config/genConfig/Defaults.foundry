    ds_type = COUNTER

[[dsFoundryL4SLBUnsuccessfulConnections]]
    ds_oid = foundryL4SLBUnsuccessfulConnections.0
    ds_type = COUNTER

[[snl4realserverportstatisticstate]]
    ds_oid	= snL4RealServerPortStatisticState.%auto-target-name%

[[snl4realserverportstatuscurrentconnection]]
    ds_oid	= snL4RealServerPortStatusCurrentConnection.%auto-target-name%

[[snl4realserverportstatusfailtime]]
    ds_oid	= snL4RealServerPortStatusFailTime.%auto-target-name%

[[snl4realserverportstatuspeakconnection]]
