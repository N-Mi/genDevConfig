foundryL4SLBUnsuccessfulConnections  = .1.3.6.1.4.1.1991.1.1.4.1.25

 
snaggblcpuutil5minavg               = .1.3.6.1.4.1.1991.1.1.2.1.53.0

snl4realserverportstatuscurrentconnection       = .1.3.6.1.4.1.1991.1.1.4.24.1.1.7
